{
  "$id": "root",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer",
      "default": 0
    },
    "petId": {
      "type": "integer",
      "default": 0
    },
    "quantity": {
      "type": "integer",
      "default": 0
    },
    "shipDate": {
      "type": "string",
      "default": "2015-07-20T15:49:04-07:00",
      "format": "date-time"
    },
    "status": {
      "type": "string",
      "default": "placed"
    },
    "complete": {
      "type": "boolean",
      "default": false
    }
  },
  "required": [
    "id",
    "petId",
    "quantity",
    "shipDate",
    "status",
    "complete"
  ],
  "additionalProperties": false
}